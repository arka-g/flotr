<%= will_paginate @tags %>

<% @tags.each do |tag| %>
  <%= link_to tag do %>
    <%= image_tag tag.images.first.image.url(:large) %>
  <% end %>
<% end %>
<br>
<%= link_to 'Browse Tags', browse_path %>

<%= javascript_include_tag "arrow_nav" %>
<%= javascript_tag do %>
  setPageLimit(<%= @tags.total_pages %>);
<% end %>
