<%= will_paginate @images %>

<% @images.each do |img| %>
  <%= link_to img do %>
    <%= image_tag img.image.url(:large) %>
  <% end %>
<% end %>
<br>
<%= link_to 'Browse Tags', browse_path %>

<%= javascript_include_tag "arrow_nav" %>
<%= javascript_tag do %>
  setPageLimit(<%= @images.total_pages %>);
<% end %>
